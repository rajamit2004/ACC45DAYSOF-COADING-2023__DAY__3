#include <stdio.h>

int main() {
    int T;
    scanf("%d", &T);

    while (T--) {
        int X, Y;
        scanf("%d %d", &X, &Y);

        int games_needed = 0;
        while (X < Y) {
            X += 8;
            games_needed++;
        }

        printf("%d\n", games_needed);
    }

    return 0;
}
