#include <stdio.h>

int main() {
    int T;
    scanf("%d", &T);

    while (T--) {
        int A1, A2, B1, B2;
        scanf("%d %d %d %d", &A1, &A2, &B1, &B2);

        int net_export_A = A1 - A2;
        int net_export_B = B1 - B2;
        int net_export_C = net_export_A + net_export_B;

        if (net_export_C > 0) {
            printf("YES\n");
        } else {
            printf("NO\n");
        }
    }

    return 0;
}
